{#
/**
 * @file
 * Theme override to display a formatted Stakeholder Logo image field.
 *
 * Available variables:
 * - image: A collection of image data.
 * - image_style: An optional image style.
 * - url: An optional URL the image can be linked to.
 *
 * @see template_preprocess_image_formatter()
 */
#}
{% if url %}
  {% if url.new_window %}
    <a target="_blank" href="{{ url }}">{{ image }}</a>
  {% else %}
    <a href="{{ url }}">{{ image }}</a>
  {% endif %}
{% else %}
  {{ image }}
{% endif %}
